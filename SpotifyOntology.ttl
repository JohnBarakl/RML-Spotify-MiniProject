###  http://www.semanticweb.org/spkar/ontologies/2025/3/untitled-ontology-8#age
:age
    rdf:type    owl:DatatypeProperty ;
    rdfs:domain :SpotifyUser ;
    rdfs:range  xsd:integer .


###  http://www.semanticweb.org/spkar/ontologies/2025/3/untitled-ontology-8#artistFollowers
:artistFollowers
    rdf:type    owl:DatatypeProperty ;
    rdfs:domain :SpotifyArtist ;
    rdfs:range  xsd:integer .


###  http://www.semanticweb.org/spkar/ontologies/2025/3/untitled-ontology-8#artistPopularity
:artistPopularity
    rdf:type    owl:DatatypeProperty ;
    rdfs:domain :SpotifyArtist ;
    rdfs:range  xsd:integer .

###  http://www.semanticweb.org/spkar/ontologies/2025/3/untitled-ontology-8#danceability
:danceability
    rdf:type    owl:DatatypeProperty ;
    rdfs:domain :SpotifyTrack ;
    rdfs:range  xsd:double .


###  http://www.semanticweb.org/spkar/ontologies/2025/3/untitled-ontology-8#explicit
:explicit
    rdf:type    owl:DatatypeProperty ;
    rdfs:domain :SpotifyTrack ;
    rdfs:range  xsd:boolean .


###  http://www.semanticweb.org/spkar/ontologies/2025/3/untitled-ontology-8#listeningTime
:listeningTimeOfDay
    rdf:type    owl:DatatypeProperty ;
    rdfs:domain :SpotifyUser ;
    rdfs:range  [ rdf:type  rdfs:Datatype ;
                  owl:oneOf [ rdf:type  rdf:List ;
                              rdf:first dbr:Afternoon ;
                              rdf:rest  [ rdf:type  rdf:List ;
                                          rdf:first dbr:Morning ;
                                          rdf:rest  [ rdf:type  rdf:List ;
                                                      rdf:first dbr:Night ;
                                                      rdf:rest  rdf:nil ] ] ] ] .


###  http://www.semanticweb.org/spkar/ontologies/2025/3/untitled-ontology-8#minutesStreamed
:minutesStreamed
    rdf:type    owl:DatatypeProperty ;
    rdfs:domain :SpotifyUser ;
    rdfs:range  xsd:integer .


