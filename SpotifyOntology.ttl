    rdfs:range  xsd:integer .


#################################################################
#    Classes
#################################################################


###  http://purl.org/ontology/mo/MusicGroup
mo:MusicGroup
    rdf:type owl:Class .


###  http://purl.org/ontology/mo/SoloMusicArtist
mo:SoloMusicArtist
    rdf:type owl:Class .


###  http://www.semanticweb.org/spkar/ontologies/2025/3/untitled-ontology-8#Group
:Group
    rdf:type            owl:Class ;
    rdfs:subClassOf     mo:MusicGroup ;
    owl:equivalentClass [ owl:intersectionOf ( mo:MusicArtist [ rdf:type          owl:Restriction ;
                                                                owl:onProperty    mo:artist ;
                                                                owl:allValuesFrom mo:SoloMusicArtist ] ) ;
                          rdf:type           owl:Class ] .

:SpotifyRelease
    rdf:type            owl:Class ;
    rdfs:subClassOf     :SpotifyCollection ;
    owl:equivalentClass [ rdf:type           owl:Restriction ;
                          owl:onProperty     :releasedBy ;
                          owl:someValuesFrom :SpotifyArtist ] .


###  http://www.semanticweb.org/spkar/ontologies/2025/3/untitled-ontology-8#SpotifyAlbum
:SpotifyAlbum
    rdf:type        owl:Class ;
    rdfs:subClassOf :SpotifyRelease ;
    rdfs:subClassOf [ rdf:type       owl:Restriction ;
                      owl:onProperty mo:release_type ;
                      owl:hasValue   mo:album ] .

:SpotifySingle
    rdf:type            owl:Class ;
    owl:equivalentClass [ rdf:type         owl:Restriction ;
                          owl:disjointWith :SpotifyAlbum ] ;
    rdfs:subClassOf     :SpotifyRelease ;
    rdfs:subClassOf     [ rdf:type       owl:Restriction ;
                          owl:onProperty mo:release_type ;
                          owl:hasValue   mo:single ] .


###  http://www.semanticweb.org/spkar/ontologies/2025/3/untitled-ontology-8#SpotifyArtist
:SpotifyArtist
    rdf:type            owl:Class ;
    owl:equivalentClass [ rdf:type    owl:Class ;
                          owl:unionOf ( [ rdf:type           owl:Restriction ;
                                          owl:onProperty     :made ;
                                          owl:someValuesFrom :SpotifyTrack ] [ rdf:type           owl:Restriction ;
                                                                               owl:onProperty     :released ;
                                                                               owl:someValuesFrom :SpotifyAlbum ]
                                        [ rdf:type           owl:Restriction ;
                                          owl:onProperty     :released ;
                                          owl:someValuesFrom :SpotifySingle ] ) ] ;
    rdfs:subClassOf     :SpotifyEntry ;
    rdfs:subClassOf     mo:MusicArtist .


###  http://www.semanticweb.org/spkar/ontologies/2025/3/untitled-ontology-8#SpotifyCollection
:SpotifyCollection
    rdf:type            owl:Class ;
    owl:equivalentClass [ rdf:type           owl:Restriction ;
                          owl:onProperty     :trackNumber ;
                          owl:someValuesFrom [ rdf:type             rdfs:Datatype ;
                                               owl:onDatatype       xsd:integer ;
                                               owl:withRestrictions ( [ xsd:minExclusive 0 ] ) ] ] ;
    rdfs:subClassOf     :SpotifyEntry .


###  http://www.semanticweb.org/spkar/ontologies/2025/3/untitled-ontology-8#SpotifyEntry
:SpotifyEntry
    rdf:type owl:Class .


###  http://www.semanticweb.org/spkar/ontologies/2025/3/untitled-ontology-8#SpotifyPlaylist
:SpotifyPlaylist
    rdf:type        owl:Class ;
    rdfs:subClassOf :SpotifyCollection .


###  http://www.semanticweb.org/spkar/ontologies/2025/3/untitled-ontology-8#SpotifyTrack
:SpotifyTrack
    rdf:type            owl:Class ;
    owl:equivalentClass [ rdf:type           owl:Restriction ;
                          owl:onProperty     :madeBy ;
                          owl:someValuesFrom :SpotifyArtist ] ;
    rdfs:subClassOf     :SpotifyEntry ;
    rdfs:subClassOf     mo:Track .

:MusicStreamingPlatform
    rdf:type owl:Class .

###  http://www.semanticweb.org/spkar/ontologies/2025/3/untitled-ontology-8#SpotifyUser
:SpotifyUser
    rdf:type            owl:Class ;
    rdfs:subClassOf     :SpotifyEntry ;
    owl:equivalentClass [ owl:intersectionOf ( :MusicServiceUser [ rdf:type owl:Restriction ;
                                                                   owl:onProperty  ] ) ]

        :MusicServiceUser
        rdf:type        owl:Class .

:UserAccountType
    rdf:type owl:Class .

