###  http://www.semanticweb.org/spkar/ontologies/2025/3/untitled-ontology-8#made
:made
    rdf:type      owl:ObjectProperty ;
    owl:inverseOf :madeBy ;
    rdfs:domain   :SpotifyArtist ;
    rdfs:range    :SpotifyTrack .


###  http://www.semanticweb.org/spkar/ontologies/2025/3/untitled-ontology-8#madeBy
:madeBy
    rdf:type    owl:ObjectProperty ;
    rdfs:domain :SpotifyTrack ;
    rdfs:range  :SpotifyArtist .


###  http://www.semanticweb.org/spkar/ontologies/2025/3/untitled-ontology-8#mostPlayedArtist
:mostPlayedArtist
