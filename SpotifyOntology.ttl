:releaseDate
    rdf:type    owl:DatatypeProperty ;
    rdfs:domain :SpotifyTrack ;
    rdfs:range  xsd:dateTime .


###  http://www.semanticweb.org/spkar/ontologies/2025/3/untitled-ontology-8#spotifyUri
:spotifyUri
    rdf:type    owl:DatatypeProperty ;
    rdfs:domain :SpotifyEntry ;
    rdfs:range  xsd:string .


###  http://www.semanticweb.org/spkar/ontologies/2025/3/untitled-ontology-8#subType
:subType
    rdf:type    owl:DatatypeProperty ;
    rdfs:domain :SpotifyUser ;
    rdfs:range  [ rdf:type  rdfs:Datatype ;
                  owl:oneOf [ rdf:type  rdf:List ;
                              rdf:first :Free ;
                              rdf:rest  [ rdf:type  rdf:List ;
                                          rdf:first :Premium ;
                                          rdf:rest  rdf:nil ] ] ] .


